{
    "swagger": "2.0",
    "info": {
        "title": "快递代取 API",
        "contact": {
            "name": "silenceLamb",
            "url": "http://www.swagger.io/support",
            "email": "ooooooooooos@163.com"
        },
        "version": "1.0.1"
    },
    "host": "localhost:9090",
    "basePath": "/",
    "paths": {
        "/express/create": {
            "post": {
                "description": "这是一个新增订单接口",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "快递订单"
                ],
                "summary": "新增订单",
                "parameters": [
                    {
                        "type": "string",
                        "description": "快递单号",
                        "name": "code",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "收货地址",
                        "name": "address",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "收货日期",
                        "name": "receive_date",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "订单费用",
                        "name": "price",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "取件码",
                        "name": "receive_code",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "创建人 id",
                        "name": "create_id",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "创建人手机号",
                        "name": "create_phone",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/express/finish": {
            "put": {
                "description": "完成订单接口 - 传入接单人 identity 为完成订单 传入创建人 identity 为收货",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "快递订单"
                ],
                "summary": "完成订单",
                "parameters": [
                    {
                        "type": "string",
                        "description": "订单 id",
                        "name": "id",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "用户唯一标识",
                        "name": "identity",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/express/info": {
            "get": {
                "description": "这是一个订单详情接口",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "快递订单"
                ],
                "summary": "订单详情",
                "parameters": [
                    {
                        "type": "string",
                        "description": "订单 id",
                        "name": "id",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/express/list": {
            "get": {
                "description": "这是一个订单列表接口",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "快递订单"
                ],
                "summary": "订单列表",
                "parameters": [
                    {
                        "type": "string",
                        "description": "页码",
                        "name": "page",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "条数",
                        "name": "size",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "integer",
                        "description": "接单状态",
                        "name": "status",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "接单人",
                        "name": "receiver_id",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "创建人",
                        "name": "create_id",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/express/order": {
            "put": {
                "description": "这是一个接单接口",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "快递订单"
                ],
                "summary": "接单",
                "parameters": [
                    {
                        "type": "string",
